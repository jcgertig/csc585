<p id="notice"><%= notice %></p>
<div class="Center-Container">
  
  <div class="Absolute-Center">
  <%= @artifacts.each do |artifact| %>


<p>
  <b>Type:</b>
  <%= artifact.a_type %>
</p>

<p>
  <b>Period:</b>
  <%= artifact.period %>
</p>

<p>
  <b>Context:</b>
  <%= artifact.context %>
</p>

<p>
  <b>County:</b>
  <%= artifact.county %>
</p>

<p>
  <b>Lat:</b>
  <%= artifact.lat %>
</p>

<p>
  <b>Lon:</b>
  <%= artifact.lon %>
</p>

<p>
  <b>Desc:</b>
  <%= artifact.desc %>
</p>
<% end %>
  </div>
</div>